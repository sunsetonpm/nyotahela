{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.4"
    }
  },
  "routes": [
    { "src": "/eligibility", "dest": "/eligibility.html" },
    { "src": "/apply", "dest": "/apply.html" },
    { "handle": "filesystem" },
    
    { "src": "/callback", "dest": "/api/stk_initiate.php" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
